    "@typescript-eslint/parser": "5.62.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-es": "4.1.0",
    "eslint-plugin-filenames": "1.3.2",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jest": "27.9.0",
    "eslint-plugin-prettier": "5.2.1",
    "jest": "29.7.0",
    "kelonio": "0.6.0",
    "prettier": "3.5.1",
    "ts-jest": "29.2.5",
    "typescript": "5.7.3"
  }
}
