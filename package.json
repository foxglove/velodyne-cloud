{
  "name": "@foxglove/velodyne-cloud",
  "version": "1.0.1",
  "packageManager": "yarn@4.9.1",
  "description": "TypeScript library for converting Velodyne LIDAR packet data to point clouds",
  "license": "MIT",
  "keywords": [
    "velodyne",
    "lidar",
    "laser",
    "pointcloud"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/foxglove/velodyne-cloud.git"
  },
  "author": {
    "name": "Foxglove Technologies Inc",
    "email": "contact@foxglove.dev"
  },
  "homepage": "https://github.com/foxglove/velodyne-cloud",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc -b",
    "lint:ci": "eslint --report-unused-disable-directives .",
    "lint": "eslint --report-unused-disable-directives --fix .",
    "prepack": "yarn build",
    "prepublishOnly": "yarn lint:ci && yarn test",
    "test": "jest"
  },
  "engines": {
    "node": ">= 14"
  },
  "devDependencies": {
    "@foxglove/eslint-plugin": "0.21.0",
    "@types/jest": "29.5.14",
